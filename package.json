{
  "name": "node-version-checker",
  "version": "1.0.3",
  "description": "Checks the current version of node against the `engines` property of package.json",
  "main": "dist/index.js",
  "bin": {
    "nvc": "dist/index.js"
  },
  "scripts": {
    "test": "gulp test",
    "lint": "gulp lint",
    "checkVersion": "node dist/index",
    "build": "gulp build",
    "clean": "gulp clean"
  },
  "pre-commit": {
    "silent": false,
    "colors": true,
    "run": [
      "checkVersion",
      "lint"
    ]
  },
  "pre-push": {
    "silent": false,
    "colors": true,
    "run": [
      "test"
    ]
  },
  "keywords": [
    "node",
    "semver",
    "checker"
  ],
  "author": "Justin Dragos <justin@gobold.com>",
  "license": "MIT",
  "dependencies": {
    "find-parent-dir": "^0.3.0",
    "semver": "^5.1.0",
    "shelljs": "^0.5.3",
    "yargs": "^3.30.0"
  },
  "engines": {
    "node": "0.10.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gobold/node-version-checker"
  },
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-cli": "^6.3.13",
    "babel-core": "^6.3.13",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.4.1",
    "eslint-config-gobold": "0.0.2",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-eslint": "^1.1.1",
    "gulp-mocha": "^2.2.0",
    "n": "^2.0.2",
    "pre-commit": "^1.1.2",
    "pre-push": "^0.1.1",
    "rimraf": "^2.4.4"
  }
}
